{
  "profiles": {

    "CLI - benchmark all": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "workingDirectory": "",
      "commandLineArgs": "run --project $(SolutionDir)/H.Benchmarks.CLI/H.Benchmarks.CLI.csproj -c Release benchmark"
    },

    "CLI - debug": {
      "commandName": "Project",
      "commandLineArgs": "debug"
    },
    "CLI - nugetversion - consolidate-deps": {
      "commandName": "Project",
      "commandLineArgs": "nugetversion consolidate-deps"
    },
    "CLI - nugetversion - patch": {
      "commandName": "Project",
      "commandLineArgs": "nugetversion update H.Benchmarks.CLI=patch"
    },
    "CLI - nugetversion - minor": {
      "commandName": "Project",
      "commandLineArgs": "nugetversion update H.Benchmarks.CLI=minor"
    },
    "CLI - nugetversion - major": {
      "commandName": "Project",
      "commandLineArgs": "nugetversion update H.Benchmarks.CLI=major"
    }
  }
}